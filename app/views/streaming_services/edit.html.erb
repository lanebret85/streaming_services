<%= form_with url: "/streaming_services/#{@streaming_service.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, value: "#{@streaming_service.name}" %>
  <%= form.label :subscribed %>
  <%= form.text_field :subscribed, value: "#{@streaming_service.subscribed}" %>
  <%= form.label :logged_in %>
  <%= form.text_field :logged_in, value: "#{@streaming_service.logged_in}" %>
  <%= form.label :rating %>
  <%= form.text_field :rating, value: "#{@streaming_service.rating}" %>
  <%= form.submit "Update Streaming Service" %>
<% end %>